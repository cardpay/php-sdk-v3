{
    "name": "cardpay/php-sdk-v3",
    "version": "1.0.0",
    "description": "PHP SDK for Cardpay API v3",
    "keywords": [
        "cardpay",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://integration.cardpay.com",
    "license": "proprietary",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Cardpay\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cardpay\\" : "test/" }
    }
}