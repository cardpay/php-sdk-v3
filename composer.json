{
    "name": "cardpay/php-sdk-v3",
    "version": "3.73.6",
    "description": "PHP SDK for Unlimit API v3",
    "keywords": [
        "unlimit",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://integration.unlimit.com",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "guzzlehttp/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Cardpay\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cardpay\\test\\" : "test/" }
    }
}